{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "21.7.1"
  },
  "scripts": {
    "start": "concurrently \"graphql-codegen --config codegen.ts --watch\" \"vite\"",
    "start:dev": "concurrently \"graphql-codegen --config codegen.ts --watch\" \"env-cmd -f .env.dev vite\"",
    "start:docker": "docker compose -f compose.dev.yml rm -f && docker compose -f compose.dev.yml up --build",
    "docker:up": "docker compose -f compose.dev.yml up --build -d",
    "docker:down": "docker compose -f compose.dev.yml down",
    "docker:rm": "docker compose -f compose.dev.yml rm -f",
    "docker:logs": "docker compose -f compose.dev.yml logs -f",
    "build": "tsc && vite build",
    "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint src --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --max-warnings 0 --fix",
    "test": "vitest --",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "check": "rm -rfv node_modules/ && npm install && npm run lint:fix && npm run prettier:format && npm run build && npm run test && rm -rf dist",
    "check:docker": "docker compose -f compose.test.yml rm -f && docker compose -f compose.test.yml up --build && docker compose -f compose.test.yml rm -f",
    "check:no-reinstall": "npm run codegen && npm run prettier:format && npm run lint:fix && npm run build && npm run test && rm -rf dist",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:local": "DOTENV_CONFIG_PATH=\"./dev.env\" graphql-codegen -r dotenv/config",
    "new:page": "npx generate-react-cli component --type=page",
    "new:component": "npx generate-react-cli component"
  },
  "dependencies": {
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-select": "^2.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@tanstack/react-table": "^8.9.2",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "final-form": "^4.20.9",
    "generate-react-cli": "^8.4.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "immer": "^10.0.2",
    "lodash.kebabcase": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.13.0",
    "react-toastify": "^10.0.0",
    "sass": "^1.63.6",
    "use-immer": "^0.9.0",
    "vite-plugin-svgr": "^4.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-query": "^6.0.0",
    "@parcel/watcher": "^2.3.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/lodash.startcase": "^4.4.9",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/react-modal": "^3.16.3",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^1.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-vitest": "^0.3.10",
    "jsdom": "^24.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.19.0",
    "vitest": "^1.0.0"
  }
}
