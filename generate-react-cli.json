{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "withTypes": true,
      "withIndex": true,
      "customTemplates": {
        "component": "templates/TemplateName.tsx",
        "style": "templates/TemplateName.scss",
        "test": "templates/TemplateName.test.tsx",
        "types": "templates/TemplateName.types.tsx",
        "index": "templates/index.tsx"
      }
    },
    "page": {
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "withTypes": true,
      "withIndex": true,
      "customTemplates": {
        "component": "templates/TemplateName.tsx",
        "style": "templates/TemplateName.scss",
        "test": "templates/TemplateName.test.tsx",
        "types": "templates/TemplateName.types.tsx",
        "index": "templates/index.tsx"
      }
    }
  },
  "usesStyledComponents": false
}
